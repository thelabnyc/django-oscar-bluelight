{
    "plugins": [
        "@babel/plugin-syntax-dynamic-import"
    ],
    "presets": [
        ["@babel/preset-env", {
            "useBuiltIns": "entry",
            "corejs": 3,
            "targets": {
                "browsers": [
                    "defaults",
                    "last 2 versions",
                    "IE 11",
                    "last 5 ios_saf versions"
                ]
            }
        }],
        "@babel/preset-react"
    ]
}
